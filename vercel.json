{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "builds": [
    { "src": "api/analyze.py", "use": "@vercel/python" },
    { "src": "frontend/package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/analyze.py" },
    { "src": "/(.*)", "dest": "frontend/$1" }
  ]
} 